<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_sync_attendance_devices" model="ir.cron">
            <field name="name">Sync Attendance Devices</field>
            <field name="model_id" ref="model_attendance_device"/>
            <field name="state">code</field>
